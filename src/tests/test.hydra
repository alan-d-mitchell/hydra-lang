func fibonacci(int n) -> int {
    if (n == 0) {
        return 0;
    }

    return fibonacci(n - 1) + fibonacci(n - 2);
}

func main() -> void {
    int pos = fibonacci(NUMBER);

    stdout("%d\n", pos);
}