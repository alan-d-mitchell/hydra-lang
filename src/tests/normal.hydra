
global const int MAX_SIZE = 100;
global string message = "Hello Hydra!";

func factorial(int n) -> int {
    if (n <= 1) {
        return 1;
    }

    return n * factorial(n - 1);
}

func processArray(int[] numbers) -> int {
    int sum = 0;

    forEach (int num in numbers) {
        sum += num;
    }

    return sum;
}

func main() -> void {
    int result = factorial(5);
    int[] data = {1, 2, 3, 4, 5};
    int total = processArray(data);

    string greeting = message;

    for (int i = 0; i < MAX_SIZE; i++) {
        break if (i == 10);

    }
}